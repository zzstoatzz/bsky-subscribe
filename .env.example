# Surge API credentials
# Get these from your Surge account dashboard
SURGE_API_KEY=your_api_key_here
SURGE_ACCOUNT_ID=your_account_id_here

# Your contact information for text messages
# Phone number MUST include country code with + prefix (e.g., +1 for US)
# IMPORTANT: The + prefix is REQUIRED for the API to work correctly
SURGE_MY_PHONE_NUMBER=+1234567890
SURGE_MY_FIRST_NAME=YourFirstName
SURGE_MY_LAST_NAME=YourLastName 